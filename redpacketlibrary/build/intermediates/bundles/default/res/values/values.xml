<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="attach_red_packet">Red Packet</string>
    <string name="easemob_red_packet">Easemob Red Packet</string>
    <string name="exclusive_red_packet">Exclusive Red Packet</string>
    <string name="msg_someone_take_red_packet">%1$s got your Red Packet</string>
    <string name="msg_take_red_packet">You got your own Red Packet</string>
    <string name="msg_take_someone_red_packet">You got Red Packet from %1$s</string>
    <string name="red_packet_record">Red Packet Records</string>
    <style name="RedPacketCheckStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignBaseline">@id/iv_money_icon</item>
        <item name="android:layout_below">@id/tv_money_greeting</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_toRightOf">@id/iv_money_icon</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">12sp</item>
    </style>
    <style name="RedPacketGreetingStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_toRightOf">@id/iv_money_icon</item>
        <item name="android:layout_marginTop">14dp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">15sp</item>
        <item name="android:maxLength">10</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="RedPacketIconStyle">
        <item name="android:layout_width">25dp</item>
        <item name="android:layout_height">36dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginTop">15dp</item>
    </style>
    <style name="RedPacketSponsorIconStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:layout_marginTop">5dp</item>
    </style>
    <style name="RedPacketSponsorStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textColor">#9e9e9e</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_marginTop">3dp</item>
    </style>
</resources>